{
    "name": "For Later!",
    "description": "Save active tabs for later usage",
    "version": "1.0",
    "manifest_version": 2,
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "/images/icons16.png",
            "32": "/images/icons32.png",
            "48": "/images/icons48.png",
            "128": "/images/icons128.png"
        }
    },
    "permissions": ["storage", "tabs"]
}